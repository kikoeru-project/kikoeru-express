{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-prototype-builtins": "off",
      "no-process-exit": "off",
      "semi": "error"
    }
}
